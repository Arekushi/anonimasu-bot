{
  "name": "anonimasu-bot",
  "version": "1.0.0",
  "description": "Music bot for Discord made in Node.JS",
  "main": "src/index.ts",
  "scripts": {
    "start": "npm run ts-node",
    "start:dist": "node dist/index.js",

    "dev": "cross-env NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production npm run start",

    "ts-node": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "ts-node:dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts --respawn --transpile-only --poll ./src/index.ts",

    "build": "npm run rimraf:dist && npm run build:js",
    "build:types": "npm run rimraf:dist && tsc --emitDeclarationOnly && tsc-alias",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps true",
    "postinstall": "npm run build",
    "rimraf:dist": "rimraf ./dist",

    "git:log": "git log --pretty=format:\"%h - %an, %ar : %s\""
  },
  "ntl": {
    "descriptions": {
      "start": "Start the project normally",
      "start:dist": "Start the project with dist",
      "dev": "Start the project in development mode with Nodemon",
      "prod": "Start the project in production mode",
      "build": "Build the project with BabelJS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arekushi/anonimasu-bot.git"
  },
  "author": "arekushi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Arekushi/anonimasu-bot/issues"
  },
  "homepage": "https://github.com/Arekushi/anonimasu-bot#readme",
  "dependencies": {
    "@arekushii/ts-aspect": "^3.0.8",
    "@babel/runtime-corejs3": "^7.17.8",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "axios": "^0.26.1",
    "config": "^3.3.7",
    "consola": "^2.15.3",
    "core-js": "^3.21.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.3",
    "express-list-endpoints": "^6.0.0",
    "ffmpeg-static": "^5.0.0",
    "glob": "^7.2.0",
    "i18n": "^0.14.2",
    "install": "^0.13.0",
    "libsodium": "^0.7.10",
    "libsodium-wrappers": "^0.7.10",
    "moment": "^2.29.1",
    "mongoose": "^6.2.8",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "rxjs": "^7.5.5",
    "save": "^2.4.0",
    "to-boolean": "^1.0.0",
